{
    "family": "vuln-task-jonathan",
    "containerDefinitions": [
        {
            "cpu": 0,
            "dockerLabels": {
                "com.datadoghq.tags.service": "fargate-app",
                "com.datadoghq.tags.env": "staging",
                "com.docker.service.name": "fargate-app"                                
            },
            "environment": [
                {
                    "name": "DD_REMOTE_CONFIGURATION_REFRESH_INTERVAL",
                    "value": "5s"
                },
                {
                    "name": "DD_IAST_ENABLED",
                    "value": "true"
                },
                {
                    "name": "DD_APPSEC_ENABLED",
                    "value": "true"
                },
                {
                    "name": "DD_RUNTIME_METRICS_ENABLED",
                    "value": "true"
                },
                {
                    "name": "DD_SERVICE",
                    "value": "fargate-app"
                },
                {
                    "name": "DD_ENV",
                    "value": "staging"
                },              
                {
                    "name": "DD_APPSEC_SCA_ENABLED",
                    "value": "true"
                },
                {
                    "name": "DD_PROFILING_ENABLED",
                    "value": "true"
                },
                {
                    "name": "DD_TAGS",
                    "value": "\"datadog\":\"true\""
                }
            ],
            "essential": true,
            "image": "369042512949.dkr.ecr.us-east-1.amazonaws.com/vuln-app-jonathan:latest",
            "logConfiguration": {
                "logDriver": "awsfirelens",
                "options": {
                    "dd_message_key": "log",
                    "apikey": "<API_KEY>",
                    "provider": "ecs",
                    "dd_service": "fargate-app",
                    "Host": "http-intake.logs.datadoghq.com",
                    "TLS": "on",
                    "dd_source": "java",
                    "Name": "datadog"
                }
            },
            "mountPoints": [],
            "name": "vuln-app-jonathan",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "systemControls": [],
            "volumesFrom": []
        },
        {
            "cpu": 256,
            "environment": [
                {
                    "name": "ECS_FARGATE",
                    "value": "true"
                },
                {
                    "name": "DD_APM_ENABLED",
                    "value": "true"
                },
                {
                    "name": "DD_API_KEY",
                    "value": "xxxxx"
                },
                {
                    "name": "DD_SITE",
                    "value": "datadoghq.com"
                },
                {
                    "name": "DD_CONTAINER_EXCLUDE",
                    "value": "image:^aws-fargate-pause$ image:^public.ecr.aws/datadog/agent$" 
                },
                {
                    "name": "DD_PROCESS_AGENT_PROCESS_COLLECTION_ENABLED",
                    "value": "true"
                }
            ],
            "essential": false,
            "image": "public.ecr.aws/datadog/agent:latest",
            "memoryReservation": 512,
            "mountPoints": [],
            "name": "datadog-agent",
            "portMappings": [
                {
                    "containerPort": 8126,
                    "hostPort": 8126,
                    "name": "datadog-agent-8126-tcp",
                    "protocol": "tcp"
                }
            ],
            "systemControls": [],
            "volumesFrom": []
        },
        {
            "cpu": 0,
            "environment": [],
            "essential": true,
            "firelensConfiguration": {
                "options": {
                    "enable-ecs-log-metadata": "true"
                },
                "type": "fluentbit"
            },
            "image": "amazon/aws-for-fluent-bit:stable",
            "mountPoints": [],
            "name": "log_router",
            "portMappings": [],
            "systemControls": [],
            "user": "0",
            "volumesFrom": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::369042512949:role/vuln-handson-taskrole",
    "executionRoleArn": "arn:aws:iam::369042512949:role/ecsTaskExecutionRole-jonathan",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024",
    "pidMode": "task",    
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}
